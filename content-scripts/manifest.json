{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "0.0.1",
  "description": "插件描述",
  "author": "tuntun",
	"host_permissions": [
		"https://*/*"
	],
	"content_scripts": [ // 配置content scripts
		{
			"matches": [
				"*://www.bilibili.com/*"
			],
			"js": [
				"content-script.js"
			],
			"css": [
				"insert-css.css"
			]
		}
	],
	"permissions": [],
	"action": {
		"default_icon": {
			"16": "img/icon.png",
			"32": "img/icon.png",
			"48": "img/icon.png",
			"64": "img/icon.png",
			"128": "img/icon.png"
		}
	},
	"icons": {
		"16": "img/icon.png",
		"32": "img/icon.png",
		"48": "img/icon.png",
		"64": "img/icon.png",
		"128": "img/icon.png"
	}
}